<p id="notice"><%= notice %></p>

<h1>Operator Registers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @operator_registers.each do |operator_register| %>
      <tr>
        <td><%= link_to 'Show', operator_register %></td>
        <td><%= link_to 'Edit', edit_operator_register_path(operator_register) %></td>
        <td><%= link_to 'Destroy', operator_register, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Operator Register', new_operator_register_path %>
